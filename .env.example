# Google Storage Cleanup Suite Configuration

# Minimum file size to consider for cleanup (MB)
MIN_FILE_SIZE_MB=5

# Face recognition tolerance (0.0-1.0, lower = stricter)
FACE_RECOGNITION_TOLERANCE=0.6

# Image similarity threshold for duplicate detection (0.0-1.0)
SIMILARITY_THRESHOLD=0.95

# Batch processing size
BATCH_SIZE=100

# Gmail batch delete size (max 1000)
GMAIL_BATCH_DELETE_SIZE=100

# Gmail categories to clean (comma-separated)
GMAIL_CATEGORIES_TO_CLEAN=SPAM,CATEGORY_PROMOTIONS,CATEGORY_SOCIAL